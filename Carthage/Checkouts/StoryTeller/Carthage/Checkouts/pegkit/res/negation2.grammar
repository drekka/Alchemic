@before {
        PKTokenizer *t = self.tokenizer;

        // symbols
        [t.symbolState add:@"{%"];
        [t.symbolState add:@"%}"];
}

document = any+;
any      = tag | text;

text     = ~tagStart;

tag      = tagStart tagContent* tagEnd;

tagStart    = '{%';
tagEnd      = '%}';

tagContent  = ~tagEnd;
